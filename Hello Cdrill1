~$ mkdir hello
~$ ls
 Desktop   Documents   Downloads   hello  'Hello Cdrill1'   Music   Pictures   Public   snap   Templates   Videos
~$ cd hello
~/hello$ mkdir five
~/hello$ ls
five
~/hello$ cd five
~/hello/five$ mkdir six
~/hello/five$ ls
six
~/hello/five$ cd six
~/hello/five/six$ touch c.txt
~/hello/five/six$ mkdir seven
~/hello/five/six$ ls
c.txt  seven
~/hello/five/six$ cd seven
~/hello/five/six/seven$ touch error.log
~/hello/five/six/seven$ ls
error.log
~/hello/five/six/seven$ cd ..
~/hello/five/six$ cd ..
~/hello/five$ cd ..
~/hello$ tree
.
└── five
    └── six
        ├── c.txt
        └── seven
            └── error.log

3 directories, 2 files
~/hello$ mkdir one
~/hello$ ls
five  one
~/hello$ cd one
~/hello/one$ touch a.txt b.txt
~/hello/one$ mkdir two
~/hello/one$ ls
a.txt  b.txt  two
~/hello/one$ cd two
~/hello/one/two$ touch d.txt
~/hello/one/two$ mkdir three
~/hello/one/two$ ls
d.txt  three
~/hello/one/two$ cd three
~/hello/one/two/three$ touch e.txt
~/hello/one/two/three$ mkdir four
~/hello/one/two/three$ ls
e.txt  four
~/hello/one/two/three$ cd four
~/hello/one/two/three/four$ touch access.log
~/hello/one/two/three/four$ ls
access.log
~/hello/one/two/three/four$ cd ..
~/hello/one/two/three$ cd ..
~/hello/one/two$ cd ..
~/hello/one$ cd ..
~/hello$ tree
